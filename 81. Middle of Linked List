Bruteforce:
class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        int count=0;
        ListNode *p = head;
        while(p!=NULL){
            count++;
            p=p->next;
        }
        int cnt = count/2;
        p=head;
        while(cnt--){
            p=p->next;
        }
        return p;
        
    }
};
T.C. = o(n)+o(n)

optimal:
class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        ListNode *slow = head, *fast = head;
        while(fast!=NULL && fast->next!=NULL){
            slow=slow->next;
            fast=fast->next->next;
        }
        return slow;
   }
};
T.C.=o(n/2)
